{
  "llvm-target": "x86_64-unknown-none",
  "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128",
  "arch": "x86_64",
  "vendor": "nonos",
  "os": "none",
  "env": "",
  "abi": "",
  "target-endian": "little",
  "target-pointer-width": "64",
  "target-c-int-width": 32,

  "cpu": "x86-64",
  "features": "+sse2,+sse3,+ssse3,+sse4.1,+sse4.2,+fxsr,+xsave,+xsaveopt,+xsavec,+xsaveerr,+x87,+mmx,+cmov,+cx8,+cx16,+popcnt,+clflush,+clflushopt,+rdtscp,+lahf-lm",

  "panic-strategy": "abort",
  "executables": true,
  "dynamic-linking": false,

  "linker": "rust-lld",

  "pre-link-args": {
    "ld.lld": [
      "-Tlinker.ld",
      "-nostdlib",
      "-nostartfiles",
      "-static",
      "-z", "relro",
      "-z", "now",
      "-z", "noexecstack",
      "--gc-sections",
      "--build-id=none"
    ]
  },

  "archive-format": "gnu",
  "emit-debug-gdb-scripts": false,

  "relocation-model": "static",
  "code-model": "kernel",
  "disable-redzone": true,
  "frame-pointer": "always",
  "function-sections": true
}
